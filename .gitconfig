[alias]
	lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
	co = checkout
	s = status
	b = branch
	pr = pull -S --rebase
 	c = commit -S
	ca = commit -S --amend
	cp = cherry-pick -S
	dfiles = diff-tree --no-commit-id --name-only -r
